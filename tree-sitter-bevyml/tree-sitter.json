{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bevyml",
      "camelcase": "BevyML",
      "title": "BevyML",
      "scope": "source.bevyml",
      "file-types": ["bevyml"],
      "injection-regex": "^bevyml$",
      "class-name": "TreeSitterBevyml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A custom bevy markup language for building UI's using HTML+CSS",
    "authors": [
      {
        "name": "Armand Burger",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-bevyml",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
